<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="#ff0176c9"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="#ff0176c9" />
            <corners android:radius="@dimen/padding_five" />
        </shape>
    </item>
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ff68b2fa" android:endColor="#ff0176c9" android:angle="0.0" />
            <corners android:radius="@dimen/padding_five" />
        </shape>
    </item>
</ripple>